{:zaal.components.pedestal/server {:service-map {:env :dev
                                                 :io.pedestal.http/routes #ig/ref :zaal.pedestal/app
                                                 :io.pedestal.http/resource-path "/public"
                                                 :io.pedestal.http/type :jetty
                                                 :io.pedestal.http/port 3000
                                                 :io.pedestal.http/join? false
                                                  ;; all origins are allowed in dev mode
                                                 :io.pedestal.http/dev-allowed-origins {:creds true :allowed-origins (constantly true)}
                                                 ;; Content Security Policy (CSP) is mostly turned off in dev mode
                                                 :io.pedestal.http/secure-headers  {:content-security-policy-settings {:object-src "'none'"}}}}
 :zaal.pedestal/app {:datomic #ig/ref :zaal.components.datomic-dev-local/db}
 :zaal.components.datomic-dev-local/db {:server-type :dev-local
                                        :system "zaal-dev"
                                        :db-name "zaal-db"
                                        :storage-dir :mem}}